{
  "container-use": {
    "type": "stdio",
    "command": "container-use",
    "args": [
      "stdio"
    ],
    "env": {}
  },
  "context7": {
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp@latest"
    ]
  },
  "deepwiki": {
    "command": "npx",
    "args": [
      "mcp-remote",
      "https://mcp.deepwiki.com/sse"
    ]
  },
  "markdownify": {
    "command": "node",
    "args": [
      "{ABSOLUTE PATH TO FILE HERE}/dist/index.js"
    ],
    "env": {
      "UV_PATH": "/path/to/uv"
    }
  },
  "chrome-devtools": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "chrome-devtools-mcp@latest"
    ],
    "env": {}
  },
  "sequential-thinking": {
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-sequential-thinking"
    ]
  }
}
