{
  "container-use": {
    "type": "stdio",
    "command": "container-use",
    "args": [
      "stdio"
    ],
    "env": {}
  },
  "context7": {
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp@latest"
    ]
  },
  "voicevox": {
    "command": "npx",
    "args": [
      "-y",
      "@t09tanaka/mcp-simple-voicevox"
    ]
  },
  "deepwiki": {
    "command": "npx",
    "args": [
      "mcp-remote",
      "https://mcp.deepwiki.com/sse"
    ]
  },
  "playright": {
    "command": "npx",
    "args": [
      "@playwright/mcp@latest"
    ]
  },
  "markdownify": {
      "command": "node",
      "args": [
        "{ABSOLUTE PATH TO FILE HERE}/dist/index.js"
      ],
      "env": {
        "UV_PATH": "/path/to/uv"
      }
    }
}
